<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean name="/index.do" class="com.netease.controller.IndexController" />
	<bean name="/login.do" class="com.netease.controller.LoginController" />
	<bean name="/logout.do" class="com.netease.controller.LogoutController" />
	<bean name="/submit.do" class="com.netease.controller.SubmitController" />

	<bean id="internalResourceViewResolver"
		class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="prefix" value="/" />
		<property name="suffix" value=".jsp" />
	</bean>
</beans>